    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/app/**/*.{js,jsx,ts,tsx}",
      "!src/app/**/*.story.{js,jsx,ts,tsx}",
      "!src/app/**/*.test.{js,jsx,ts,tsx}",
      "!src/app/config.{js,jsx,ts,tsx}"
    ]
  },
