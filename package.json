  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "coverage": "react-scripts test --env=jsdom --coverage --no-cache",
    "storybook": "start-storybook -s ./public -p 9001 -c .storybook",
